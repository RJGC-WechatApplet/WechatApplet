page{
  background-color: honeydew;
}
/*搜索框*/
.search_input{
  background-color: #2E8B57;
  height: 90rpx;
  padding: 10rpx;
}
.nav_search{
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: white;
  border-radius: 15rpx;
}

/*轮播图*/
.index_swiper{
  padding: 10rpx;
  width:100%;
  height: 100rpx;
}

/*首页导航*/
.nav{
  white-space: nowrap;
  padding: 5rpx 0;
}
.nav_item{
  padding: 20rpx 50rpx;
  font-size: 45rpx;
  display: inline-block;
}
.nav_item.active{
  border-bottom: 7rpx solid red;
}